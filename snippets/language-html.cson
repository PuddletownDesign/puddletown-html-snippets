# Custom Form elements
## Html Snippets
'.text.html':
    'Javascript External':
        'prefix': 'js'
        'body': '<script src="$1.js" defer></script>$0'
    'CSS External':
        'prefix': 'css'
        'body': '<link href="$1.css" rel="stylesheet">$0'
    'Structure':
        'prefix': 'html'
        'body': """
                  <!DOCTYPE html>
                  <html lang="en">
                  <head>
                  <meta charset="utf-8">
                  <meta name="viewport" content="width=device-width, initial-scale=1.0">
                  <link href="css/styles.css" rel="stylesheet">
                  <title>$1</title>
                  </head>
                  <body>

                  $0

                  </body>
                  </html>
                """
    'Form Wrapper':
        'prefix': 'form'
        'body': '<form action="${1:#}" method="${2:post}"${3: enctype="multipart/form-data"}>\n$0\n</form>'
    'Fieldset':
        'prefix': 'fieldset'
        'body': '<fieldset>\n\t$0\n</fieldset>'
    'Text Input':
        'prefix': 'input-1-text'
        'body': '<label for="$1">$1</label><br>\n<input type="text" name="$1" id="$1"${2: ${3:placeholder="$4"}} ${5: ${6:value="$7"}} ${8:required}>\n<br><br>\n$0'
    'Text Area':
        'prefix': 'input-2-textarea'
        'body': '<label for="$1">$1</label><br>\n<textarea name="$1" id="$1" rows="8" cols="40" ${2: ${3:placeholder="$4"}} ${5:required}></textarea><br><br>\n$0\n'
    'Submit Input':
        'prefix': 'input-submit'
        'body': '<input type="submit" id="submit" value="${1:submit}">$0'
    'Hidden Input':
        'prefix': 'input-3-hidden'
        'body': '<input type="hidden" name="$1"${2: value="$3"}>$0'
    'Password Input':
        'prefix': 'input-4-password'
        'body': '<label for="password">Password</label><br>\n<input type="password" id="password" name="password"><br><br>\n$0\n'
    'Checkbox Input':
        'prefix': 'input-6-checkbox'
        'body': '<input type="checkbox" name="$1" id="$1">\n<label for="$1">$1</label><br>\n$0'
    'Radio Button Input':
        'prefix': 'input-5-radio'
        'body': '<input type="radio" name="$2" id="$1" value="$2">\n<label for="$1">$2</label>\n<br>\n$0'
        'Select Menu Input':
            'prefix': 'input-select'
            'body': '<label for="$1">$1</label><br>\n<select name="$1" id="$1">\n<option value="$2">$2</option>$0\n</select>\n<br><br>'
    'Option Group':
        'prefix': 'optgroup'
        'body': '<optgroup label="$1">\n$0\n</optgroup>'
    'Option':
        'prefix': 'input-option'
        'body': '<option value="${1:}">${1:}</option>\n$0'
    'Search Input':
        'prefix': 'input-7-search'
        'body': '<label for="$1">$1</label><br>\n<input type="search" name="$1" id="$1" placeholder="$1" role="search">\n<br><br>\n$0\n'
    'Email Input':
        'prefix': 'input-email'
        'body': '<label for="email">Email Addess</label><br>\n<input type="email" name="email" placeholder="Email Address" ${1:required}>\n<br><br>\n\n$0'
    'Input Phone':
        'prefix': 'input-phone'
        'body': '<label for="tel">Telephone</label><br>\n<input type="tel" name="tel" ${1:required}>\n<br><br>\n\n$0'
    'Input File':
        'prefix': 'input-8-file'
        'body': '<label for="$1">$1</label><br>\n<input type="file" name="$1" id="$1"><br><br>\n$0'
        'enocoding':
            'prefix': 'Multipart form encoding'
            'body': 'enctype="multipart/form-data"'
    'PlaceHolder Site':
        'prefix': 'temp'
        'body': """
<!DOCTYPE html><html lang="en"><head><meta charset="utf-8"><meta name="viewport" content="width=device-width; initial-scale=1.0">
<title>Index</title>
<style type="text/css">
html,body{margin:0;padding:0}html{position:relative;height:100%;background:#000111 url(data:image/svg+xml;base64,) center;background-size:10em}body{width:100%;height:100%;background-color:rgba(0,1,17,0.7)}body::before,body::after{position:absolute;content:" ";left:0px;width:100%;height:50%}body::before{top:0;background:linear-gradient(to bottom, #000111 0%,rgba(0,1,17,0.2) 100%)}body::after{bottom:0;background:linear-gradient(to bottom, rgba(0,1,17,0.2) 0%,#000111 100%)}
</style></head><body></body></html>
        """

